// Load script to generate combinations of specified characters and store in a variable in Qlik
Let vChars = '&,*,-,+,=,|,<,>,/,';
Let vNumChars = SubStringCount(vChars, ',') + 1;
Let vCombinations = '';

// Create an inline table with the characters
Characters:
LOAD * INLINE [
Char
$(vChars)
];

// Cartesian Product to generate all combinations
Combinations:
LOAD
  Char & Char as Combination
RESIDENT Characters
JOIN (Characters)
;

// Load the combinations into a variable
FOR i = 0 TO NoOfRows('Combinations') - 1
  Let vCombinations = vCombinations & Peek('Combination', i, 'Combinations') & Chr(10);
END FOR

// Drop the temporary table
DROP TABLE Characters;
DROP TABLE Combinations;

// Output the variable content
TRACE $(vCombinations);
