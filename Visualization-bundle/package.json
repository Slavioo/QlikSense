{
  "name": "@qlik-trial/visualization-bundle",
  "version": "2.75.0",
  "description": "Visualization bundle",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "author": "",
  "license": "UNLICENSED",
  "files": [
    "build/bundle/**/*",
    "build/manifest.json"
  ],
  "scripts": {
    "build": "npm install && gradlew -q build",
    "updateVersions": "gradlew -q updateVersions",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:setup:selenium": "npx webdriver-manager clean && npx webdriver-manager update --gecko=false",
    "test:setup:docker": "docker-compose up -d --build",
    "test:integration": "aw protractor -c test/integration/aw.config.js"
  },
  "dependencies": {
    "@nebula.js/sn-funnel-chart": "1.5.3",
    "@nebula.js/sn-grid-chart": "1.10.5",
    "@nebula.js/sn-org-chart": "0.15.0",
    "@nebula.js/sn-sankey-chart": "1.6.5",
    "@nebula.js/sn-network-chart": "0.2.0",
    "@nebula.js/sn-table": "2.2.2"
  },
  "devDependencies": {
    "@after-work.js/aw": "^6.0.10",
    "@babel/core": "^7.6.4",
    "after-work.js": "4.3.2",
    "babel-core": "^6.26.3",
    "babel-plugin-istanbul": "^5.0.1",
    "enigma.js": "^2.3.0",
    "eslint": "5.5.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-mocha": "5.2.0",
    "eslint-plugin-protractor": "1.41.0",
    "eslint-plugin-react": "7.11.1",
    "jsdom": "^12.0.0",
    "protractor": "^5.3.0"
  }
}
