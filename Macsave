Sub ExportTablesToCSV
    For Each obj In ActiveDocument.GetSheetObjectArray
        If obj.Type = "ChartObject" Then ' Changed to ChartObject to exclude non-table objects
            If obj.GetCaption.Name = "CH1" Or obj.GetCaption.Name = "CH3" Then
                Set table = obj.GetTableObject
                table.SelectAll
                ActiveDocument.GetApplication.WaitForIdle
                ActiveDocument.Export "CSV", False
                ActiveDocument.GetApplication.WaitForIdle
                table.ClearAll
            End If
        End If
    Next
End Sub
