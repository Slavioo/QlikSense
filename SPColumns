
-- Execute the stored procedure with @RequestId = 0 and store the result in a temp table
SELECT *
INTO #TempResult
FROM YourStoredProcedure
WHERE RequestId = 0

-- Query the column names from the temp table
SELECT COLUMN_NAME
FROM INFORMATION_SCHEMA.COLUMNS
WHERE TABLE_NAME LIKE '#TempResult%'

-- Clean up the temp table
DROP TABLE #TempResult
