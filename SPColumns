-- Create a temporary table to store the column names
CREATE TABLE #TempColumns
(
    ColumnNames VARCHAR(255)
)

-- Execute the stored procedure with @RequestId = 0
EXEC YourStoredProcedure @RequestId = 0

-- Insert the column names into the temporary table
INSERT INTO #TempColumns (ColumnNames)
EXEC sp_describe_first_result_set N'EXEC YourStoredProcedure @RequestId = 0', NULL, 1

-- Query the temporary table to retrieve the column names
SELECT ColumnNames
FROM #TempColumns

-- Clean up the temporary table
DROP TABLE #TempColumns
