DECLARE @ProcedureName NVARCHAR(128) = 'YourStoredProcedureName'; -- Replace with the name of your stored procedure

-- Create a temporary table to hold the result set
CREATE TABLE #ColumnNames (
    ColumnName NVARCHAR(128)
);

-- Insert the result set columns into the temporary table
INSERT INTO #ColumnNames
EXEC sp_executesql N'EXEC ' + QUOTENAME(@ProcedureName);

-- Display the contents of the temporary table
SELECT ColumnName
FROM #ColumnNames;

