-- Declare a variable to store the column names
DECLARE @ColumnNames VARCHAR(MAX)

-- Execute the stored procedure to retrieve the result set
EXEC YourStoredProcedure @Parameter1 = 'Value1', @Parameter2 = 'Value2';

-- Query the metadata of the result set and store column names in the variable
SELECT @ColumnNames = STRING_AGG(name, ', ')
FROM sys.dm_exec_describe_first_result_set_for_object(OBJECT_ID(N'YourStoredProcedure'), 1)

-- Output the column names
SELECT @ColumnNames AS ColumnNames
