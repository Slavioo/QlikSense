// Replace 'YourStoredProcedureName' with the name of your stored procedure
let vProcedureName = 'YourStoredProcedureName';

// Retrieve the column names and data types of the stored procedure result set
ResultColumns:
SQL SELECT COLUMN_NAME as ColumnName, DATA_TYPE as DataType
FROM sys.dm_exec_describe_first_result_set_for_object(
    OBJECT_ID(''' & vProcedureName & '''),
    NULL
);

// Display the result columns
SELECT * FROM ResultColumns;
