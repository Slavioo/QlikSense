DECLARE @ProcedureName NVARCHAR(128) = 'YourStoredProcedureName';
DECLARE @CsvFilePath NVARCHAR(256) = 'C:\Path\To\Output.csv';

CREATE TABLE #TempResult (ColumnName NVARCHAR(128));

INSERT INTO #TempResult
EXEC sp_describe_first_result_set CONCAT('EXEC ', @ProcedureName);

EXEC xp_cmdshell CONCAT('bcp "SELECT ColumnName FROM #TempResult ORDER BY ColumnName" queryout "', @CsvFilePath, '" -c -t, -T -S');

DROP TABLE #TempResult;
