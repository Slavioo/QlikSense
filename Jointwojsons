# Read the original JSON content
$originalJsonPath = "path\to\original.json"
$originalJson = Get-Content $originalJsonPath | ConvertFrom-Json

# Read the JSON content with additional properties
$extendJsonPath = "path\to\extend.json"
$extendJson = Get-Content $extendJsonPath | ConvertFrom-Json

# Extend the original JSON with the additional properties
foreach ($prop in $extendJson.PSObject.Properties) {
    $originalJson | Add-Member -NotePropertyName $prop.Name -NotePropertyValue $prop.Value
}

# Convert back to JSON and save to a new file
$extendedJsonPath = "path\to\extended.json"
$originalJson | ConvertTo-Json | Set-Content $extendedJsonPath

Write-Host "The original JSON file has been extended and saved as extended.json"
